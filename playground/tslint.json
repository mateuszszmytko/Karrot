{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-any": true,
        "semicolon": true,
        "indent": false,
        "quotemark": false,
        "no-console": false,
        "prefer-method-signature": true,
        "one-variable-per-declaration": false,
        "forin": false,
        "max-classes-per-file": false,
        "ordered-imports": false,
        "variable-name": false,
        "typedef": [
            true,
            "call-signature",
            "property-declaration"
        ]
    },
    "rulesDirectory": []    
}